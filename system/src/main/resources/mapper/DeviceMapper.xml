<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lsy.mapper.DeviceMapper">
    <select id="findAllDevice" resultType="com.lsy.pojo.Device">
        SELECT * FROM t_device
    </select>
    <select id="count" resultType="java.lang.Long">
        SELECT COUNT(*) FROM t_device
    </select>
    <select id="findByPage" resultType="com.lsy.pojo.Device">
        SELECT * FROM t_device limit ${start},${pageSize}
    </select>
</mapper>